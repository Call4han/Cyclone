<idea-plugin>
    <id>me.callahan.cyclone</id>
    <name>Cyclone</name>
    <version>1.0.0</version>
    <vendor email="ccasarider@gmail.com" url="">call4han</vendor>
    <description><![CDATA[
      This is a progress bar plugin that you can put images and gifs to your liking!.
    ]]></description>
    <depends>com.intellij.modules.platform</depends>
    <extensions defaultExtensionNs="com.intellij">

        <applicationConfigurable
                parentId="editor"
                displayName="Example"
                instance="me.callahan.cyclone.settings.SettingsConfigurableExample"
                id="me.callahan.cyclone.settings.SettingsConfigurableExample"/>
        <applicationService serviceImplementation="me.callahan.cyclone.settings.ConfigurableState"/>
    </extensions>

    <application-components>
        <component>
            <implementation-class>me.callahan.cyclone.ui.ProgressBarListener</implementation-class>
        </component>
    </application-components>
</idea-plugin>